{
    "name": "softspring/polymorphic-form-type",
    "description": "Symfony Polymorphic Form Type",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "symfony/form": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\PolymorphicFormType\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\PolymorphicFormType\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
