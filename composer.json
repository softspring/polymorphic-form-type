{
    "name": "softspring/polymorphic-form-type",
    "description": "Symfony Polymorphic Form Type",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/form": "^5.4 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\PolymorphicFormType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\PolymorphicFormType\\Tests\\": "tests/"
        }
    },
    "extra": {
    }
}
