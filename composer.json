{
    "name": "softspring/polymorphic-form-type",
    "description": "Symfony Polymorphic Form Type",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1",
        "symfony/form": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.11",
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\PolymorphicFormType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\PolymorphicFormType\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
